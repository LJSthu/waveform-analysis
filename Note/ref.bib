	volume = {100},
	issn = {2470-0010, 2470-0029},
	shorttitle = {{XENON1T} {Dark} {Matter} {Data} {Analysis}},
	url = {http://arxiv.org/abs/1906.04717},
	doi = {10.1103/PhysRevD.100.052014},
	abstract = {The XENON1T experiment at the Laboratori Nazionali del Gran Sasso is the most sensitive direct detection experiment for dark matter in the form of weakly interacting particles (WIMPs) with masses above \$6{\textbackslash},\$GeV/\$c{\textasciicircum}2\$ scattering off nuclei. The detector employs a dual-phase time projection chamber with 2.0 metric tons of liquid xenon in the target. A one metric \${\textbackslash}mathrm\{ton\}{\textbackslash}times{\textbackslash}mathrm\{year\}\$ exposure of science data was collected between October 2016 and February 2018. This article reports on the performance of the detector during this period and describes details of the data analysis that led to the most stringent exclusion limits on various WIMP-nucleon interaction models to date. In particular, signal reconstruction, event selection and calibration of the detector response to nuclear and electronic recoils in XENON1T are discussed.},
	number = {5},
	urldate = {2020-10-11},
	journal = {Physical Review D},
	author = {Aprile, E. and others},
    authortype = {XENON Collaboration}, 
	month = sep,
	year = {2019},
	note = {Number: 5
arXiv: 1906.04717},
	keywords = {High Energy Physics - Experiment, Physics - Instrumentation and Detectors},
	pages = {052014},
	file = {arXiv Fulltext PDF:/Users/xudacheng/Zotero Waveform/storage/AA39FQBE/Aprile et al. - 2019 - XENON1T Dark Matter Data Analysis Signal Reconstr.pdf:application/pdf;arXiv.org Snapshot:/Users/xudacheng/Zotero Waveform/storage/5VGQZJVT/1906.html:text/html}
}
