@article{lagomarsino_gateless_1999,
	title = {A gateless charge integrator for {Borexino} energy measurement},
	volume = {430},
	issn = {0168-9002},
	url = {http://www.sciencedirect.com/science/article/pii/S0168900299001709},
	doi = {10.1016/S0168-9002(99)00170-9},
	abstract = {A gateless charge integrator designed for the energy measurement in the Borexino experiment is described and the results of various tests performed on prototypes are shown. The circuit integrates always its input taking advantage of the AC coupling to the photomultipliers that equalizes to zero the charge associated with each event signal. A double sampling of the integrator output allows to perform a charge measurement in principle without dead time, avoiding the use of gate signals and baseline restoration networks and permitting a precise detection of the fast correlated events (delayed coincidences due to the decay of a nuclide producing a daughter having a lifetime in the tens of ns time range). The precision of the charge measurement is discussed together with the performances of the front end Borexino board where the integrator is mounted.},
	language = {en},
	number = {2},
	urldate = {2020-10-09},
	journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
	author = {Lagomarsino, V and Testera, G},
	month = jul,
	year = {1999},
	note = {Number: 2},
	pages = {435--446},
	file = {ScienceDirect Snapshot:/Users/xudacheng/Zotero Waveform/storage/PPU87LRZ/S0168900299001709.html:text/html;ScienceDirect Full Text PDF:/Users/xudacheng/Zotero Waveform/storage/LGBXW2Z4/Lagomarsino and Testera - 1999 - A gateless charge integrator for Borexino energy m.pdf:application/pdf}
}

